# sigmoid GELU approximation
{
    settings: {
        iterations: 1000
        lens: [10, 384, 3072]
#        strides: [384, 1]
        type: "float"
    },
    compile_op: {
        name: "pointwise"
        lambda: 
        '''
        [](auto x)
        {
            using x_type = decltype(x);
            x_type one = 1.;
            x_type fit_const = 2.;
            return x / (one + exp(-fit_const * x));
        }
        '''
        inputs: [{}, {}]
    }
}
